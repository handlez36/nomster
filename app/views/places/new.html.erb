<div class="col-xs-10 col-xs-offset-1 booyah-box">
  <h1>Enter Your Place Details</h1>
  <%= simple_form_for @place do |f| %>
    <%= f.input :name %><br />
    <%= f.input :description %><br />
    <%= f.input :address %>
    <br />
    <%= f.submit "Create", :class => "btn btn-primary" %>
  <% end %>
</div>
